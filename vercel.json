{
  "rewrites": [
    {
      "source": "/article/:id",
      "destination": "havaasa-backend-production.up.railway.app/article/:id"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
